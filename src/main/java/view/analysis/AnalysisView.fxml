<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane prefHeight="400.0" prefWidth="600.0" stylesheets="/style/style.css"
            AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="view.analysis.AnalysisView">
    <top>
        <Label fx:id="analysisName" text="Analysis" BorderPane.alignment="CENTER" styleClass="form_header">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
            <padding>
                <Insets bottom="18.0" left="4.0" right="4.0" top="18.0"/>
            </padding>
        </Label>
    </top>
    <center>
        <PieChart BorderPane.alignment="CENTER" fx:id="pieChart"/>
    </center>
</BorderPane>
